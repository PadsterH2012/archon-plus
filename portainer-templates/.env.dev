# Development Environment Configuration
ENVIRONMENT=development

# Supabase Configuration (Development Database)
SUPABASE_URL=https://your-dev-project.supabase.co
SUPABASE_SERVICE_KEY=your-dev-service-key

# Logging Configuration
LOG_LEVEL=DEBUG
LOGFIRE_TOKEN=

# Host Configuration
HOST=localhost

# Port Configuration (Development - 9xxx range)
ARCHON_SERVER_PORT=9181
ARCHON_MCP_PORT=9051
ARCHON_AGENTS_PORT=9052
ARCHON_UI_PORT=4737
ARCHON_EMBEDDINGS_PORT=9080

# Resource Configuration (Reduced for development)
TEI_MEMORY_LIMIT=1G
TEI_CPU_LIMIT=1
TEI_MEMORY_RESERVATION=512M
TEI_CPU_RESERVATION=0.5
TEI_MAX_CONCURRENT=256
TEI_MAX_BATCH_TOKENS=8192
TEI_MAX_BATCH_REQUESTS=16

# Model Configuration
EMBEDDING_MODEL_ID=sentence-transformers/all-MiniLM-L6-v2

# Development-specific features
ENABLE_DEBUG_ENDPOINTS=true
ENABLE_CORS=true
CORS_ORIGINS=http://localhost:4737,http://localhost:3000
